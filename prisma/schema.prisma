

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Event {
  id              Int         @id @default(uuid())
  name            String
  totalTickets    Int
  availableTickets Int
  createdAt DateTime @default(now())
  bookings        Booking[]
  waitingList     WaitingList[]
}